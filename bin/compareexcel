#!/usr/bin/bash
#
# unpack and compare two excel files
#

if [ "$#" -ne 2 ]
then
    echo "Two arguments expected"
    exit 2
fi

file1=$1
file2=$2

if [ ! -f $file1 ]
then
    echo "$file1 not found"
    exit 2
fi

if [ ! -f $file2 ]
then
    echo "$file2 not found"
    exit 2
fi

unpackexcel $file1
unpackexcel $file2

dir1=${file1%.*}
dir2=${file2%.*}

xdiff $dir1 $dir2

